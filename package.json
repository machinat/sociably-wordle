{
  "name": "wordle-machina",
  "private": true,
  "scripts": {
    "activateTwitterWebhook": "per-env",
    "activateTwitterWebhook:development": "dotenv -- ts-node ./src/cli/activateTwitterWebhook.ts",
    "activateTwitterWebhook:production": "node ./lib/cli/activateTwitterWebhook.js",
    "prepareWordlist": "per-env",
    "prepareWordlist:development": "dotenv -- ts-node ./src/cli/prepareWordlist.ts",
    "prepareWordlist:production": "node ./lib/cli/prepareWordlist.js",
    "build": "npm run clean && npm run build:src && npm run build:webview",
    "build:src": "tsc",
    "build:webview": "dotenv -- next build ./webview",
    "clean": "rm -rf ./lib ./dist && rm -f tsconfig.tsbuildinfo",
    "dev": "dotenv -- ts-node ./src/cli/dev.ts",
    "migrate": "per-env",
    "migrate:development": "dotenv -- ts-node ./src/cli/migrate.ts",
    "migrate:production": "node ./lib/cli/migrate.js",
    "start": "per-env",
    "start:development": "dotenv -- ts-node ./src/index.ts",
    "start:production": "node ./lib/index.js"
  },
  "dependencies": {
    "@diatche/parse-time": "^0.1.1",
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@machinat/core": "0.6.0-beta.1",
    "@machinat/dev-tools": "0.6.0-beta.0",
    "@machinat/dialogflow": "0.6.0-beta.0",
    "@machinat/http": "0.6.0-beta.0",
    "@machinat/line": "0.6.0-beta.4",
    "@machinat/messenger": "0.6.0-beta.5",
    "@machinat/per-env": "^1.1.0",
    "@machinat/redis-state": "0.6.0-beta.0",
    "@machinat/script": "0.6.0-beta.0",
    "@machinat/stream": "0.6.0-beta.0",
    "@machinat/telegram": "0.6.0-beta.11",
    "@machinat/twitter": "0.6.0-beta.8",
    "@machinat/webview": "0.6.0-beta.5",
    "classnames": "^2.3.1",
    "commander": "^8.3.0",
    "grapheme-splitter": "^1.0.4",
    "next": "^12.0.8",
    "react": "^17.0.2",
    "react-countdown": "^2.3.2",
    "react-dom": "^17.0.2",
    "ua-parser-js": "^1.0.2",
    "umzug": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.38",
    "autoprefixer": "^10.4.4",
    "dotenv-cli": "^4.1.1",
    "localtunnel": "^2.0.2",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
